{
    "name": "depcut",
    "version": "1.1.0",
    "description": "A CLI-based unused dependency remover.",
    "main": "lib/index.cjs",
    "scripts": {
        "start": "npm run build && node lib/index.cjs",
        "build": "tsc -b tsconfig.json && node scripts",
        "format": "prettier --write src/*.ts"
    },
    "type": "commonjs",
    "author": "Terry Kim",
    "license": "MIT",
    "keywords": [
        "unused",
        "dependency",
        "remover"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/realyoterry/depcut.git"
    },
    "devDependencies": {
        "@types/node": "^22.13.10",
        "@types/shell-quote": "^1.7.5",
        "prettier": "^3.5.1",
        "typescript": "^5.7.3"
    },
    "bin": {
        "depcut": "lib/index.cjs"
    },
    "dependencies": {
        "shell-quote": "^1.8.2"
    }
}
